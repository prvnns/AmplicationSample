datasource postgres {
  provider = "postgresql"
  url      = env("POSTGRESQL_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  createdAt DateTime @default(now())
  firstName String?
  id        String   @id @default(cuid())
  lastName  String?
  password  String
  roles     String[]
  updatedAt DateTime @updatedAt
  username  String   @unique
}

model ShapeDatum {
  createdAt   DateTime @default(now())
  description String?
  id          String   @id @default(cuid())
  imageImage  String?
  name        String?
  origin      String?
  unitPrice   Float?
  unitsSold   Int?
  updatedAt   DateTime @updatedAt
}